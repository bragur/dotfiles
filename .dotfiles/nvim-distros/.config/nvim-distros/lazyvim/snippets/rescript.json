{
  "Rescript Module": {
    "prefix": "resm",
    "body": "module ${1:ModuleName} = {\n\t$2\n}"
  },
  "Rescript Module With Type": {
    "prefix": "resmwt",
    "body": [
      "module ${1:ModuleName} = {",
      "  type t = $2",
      "  let make = ($3) => {",
      "    $4",
      "  }",
      "}"
    ]
  },
  "Rescript-React component module": {
    "prefix": "rcm",
    "body": [
      "module ${1:ModuleName} = {",
      "  @react.component",
      "  let make = ($2) => {",
      "    $3",
      "  }",
      "}"
    ],
    "description": "Rescript React component module"
  },
  "File level Rescript-React component": {
    "prefix": "flc",
    "body": ["@react.component", "let make = ($1) => {", "  $2", "}"],
    "description": "Rescript React component module"
  },
  "Rescript-React String": {
    "prefix": "rrs",
    "body": ["{${1:${TM_SELECTED_TEXT}}->React.string}"]
  },
  "Rescript-React String with Double Quotes": {
    "prefix": "rrsdq",
    "body": ["{\"${1:${TM_SELECTED_TEXT}}\"->React.string}"]
  },
  "Rescript-React String with backtick": {
    "prefix": "rrsb",
    "body": ["{`${1:${TM_SELECTED_TEXT}}`->React.string}"]
  },
  "Rescript-React UseState": {
    "prefix": "rrus",
    "body": [
      "let (${1:state}, set${1/(.*)/${1:/pascalcase}/}) = React.useState(() => ${2:initialState})"
    ]
  },
  "Rescript-React UseMemo0": {
    "prefix": "rrum0",
    "body": ["React.useMemo0(() => {", "  $1", "})"]
  },
  "Rescript-React UseMemo1": {
    "prefix": "rrum1",
    "body": ["React.useMemo1(() => {", "  $1", "}, [${2:dependency}])"]
  },
  "Rescript-React UseMemo2": {
    "prefix": "rrum2",
    "body": [
      "React.useMemo2(() => {",
      "  $1",
      "}, (${2:dependency1},${3:dependency2}))"
    ]
  },
  "Rescript-React UseMemo3": {
    "prefix": "rrum3",
    "body": [
      "React.useMemo3(() => {",
      "  $1",
      "}, (${2:dependency1},${3:dependency2},${4:dependency3}))"
    ]
  },
  "Rescript-React UseMemo4": {
